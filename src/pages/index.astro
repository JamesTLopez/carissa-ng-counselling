---
import Layout from '../layouts/Layout.astro';
import Header from '../components/Header.astro'
import Landing from '../components/sections/Landing.astro';
import Services from '../components/sections/Services.astro';
import About from '../components/sections/About.astro';
import Footer from 'src/components/Footer.astro';
import Contact from 'src/components/sections/Contact.astro';
---
<Layout title="Carissa Ng Counselling">
	<main class="relative flex flex-col min-h-screen mx-auto">
		<Header/>
		<Landing/>
		<img class="aspect-video w-full object-cover object-center h-[375px] brightness-50" src={"https://images.unsplash.com/photo-1566044366987-6de6ca566441?q=80&w=2070&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D"} alt="Peaceful" />
		<Services/>
		<About/>
		
		<img class="aspect-video w-full object-cover object-center h-[375px] brightness-50" src={"https://images.unsplash.com/photo-1619441207978-3d326c46e2c9?q=80&w=2669&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D"} alt="Peaceful" />
		<Contact/>
		<Footer/>
	</main>
</Layout>
