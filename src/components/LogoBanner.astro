---
import { Image } from "astro:assets";

const { banner } = Astro.props;
---

<div class="relative w-full h-full min-h-[375px]">
  <Image
    class="absolute inset-0 aspect-video h-full w-full object-cover object-center brightness-50"
    src={banner}
    width={"1280"}
    height={"720"}
    alt="Peaceful"
  />
</div>
